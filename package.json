{
  "name": "CE-Deploy",
  "version": "1.0.2",
  "description": "Deployment app for Cisco Video Endpoints",
  "main": "lib/main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron ./lib/main",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": "https://github.com/voipnorm/InRoom-Macro-Deployer.git",
  "keywords": [],
  "author": "Chris Norman",
  "license": "MIT",
  "devDependencies": {
    "electron": "^4.0.2",
    "electron-builder": "^20.38.5",
    "electron-packager": "^13.0.1",
    "electron-winstaller": "^2.7.0",
    "path": "^0.12.7",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "cisco-tp-client": "^0.1.0",
    "electron-log": "^3.0.1",
    "exceljs": "^1.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "request": "^2.88.0"
  },
  "build": {
    "electronDownload": {
      "cache": "...path..."
    },
    "appId": "com.electron.CEDeploy",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  }
}
